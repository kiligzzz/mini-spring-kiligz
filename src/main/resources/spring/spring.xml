<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--  Spring的配置文件，主要配置和业务逻辑有关的  -->

    <bean id="kiligzDao" name="kiligzDao" class="com.kiligz.test.KiligzDao"/>

    <bean id="kiligzService" name="kiligzService" class="com.kiligz.test.KiligzService"
          init-method="init" destroy-method="xmlDestroy">
        <property name="prefix" value="test:"/>
        <property name="kiligzDao" ref="kiligzDao"/>
    </bean>

    <bean class="com.kiligz.test.KiligzBeanFactoryPostProcessor"/>
    <bean class="com.kiligz.test.KiligzBeanPostProcessor"/>
</beans>